{
    "name": "mu/files-module",
    "version": "1.2.0",
    "description": "MUFilesModule module generated by ModuleStudio 1.0.2.",
    "type": "zikula-module",
    "license": "LGPL-3.0+",
    "authors": [
        {
            "name": "Michael Ueberschaer",
            "email": "info@homepages-mit-zikula.de",
            "homepage": "https://homepages-mit-zikula.de",
            "role": "owner"
        }
    ],
    "autoload": {
        "psr-4": { "MU\\FilesModule\\": "" }
    },
    "require": {
        "php": ">=5.5.9"
    },
    "require-dev": {
        "jakub-onderka/php-parallel-lint": "*",
        "phploc/phploc": "*",
        "pdepend/pdepend": "2.*",
        "squizlabs/php_codesniffer": "*",
        "phpunit/phpunit": "4.*",
        "theseer/phpdox": "*",
        "phpmd/phpmd": "2.*",
        "sebastian/phpcpd": "*"
    },
    "extra": {
        "zikula": {
            "core-compatibility": ">=1.5.1 <3.0",
            "class": "MU\\FilesModule\\MUFilesModule",
            "displayname": "Files",
            "url": "files",
            "capabilities": {
                "user": {"route": "mufilesmodule_collection_index"},
                "admin": {"route": "mufilesmodule_collection_adminindex"},
                "categorizable": {
                    "entities": [
                        "MU\\FilesModule\\Entity\\CollectionEntity"
                    ]
                }
            },
            "securityschema": {
                "MUFilesModule::": "::",
                "MUFilesModule:ItemListBlock:": "Block title::",
                "MUFilesModule:ModerationBlock:": "Block title::",
                "MUFilesModule:Collection:": "Collection ID::",
                "MUFilesModule:Collection:Collection": "Collection ID:Collection ID:",
                "MUFilesModule:File:": "File ID::",
                "MUFilesModule:Collection:File": "Collection ID:File ID:",
                "MUFilesModule::Ajax": "::"
            }
        }
    },
    "config": {
        "vendor-dir": "vendor",
        "preferred-install": "dist",
        "optimize-autoloader": true,
        "platform": {
            "php": "5.5.9"
        }
    }
}
