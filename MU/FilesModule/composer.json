{
	"name" : "mu/files-module",
	"version" : "1.2.0",
	"description" : "MUFilesModule generated by ModuleStudio 1.4.0.",
	"type" : "zikula-module",
	"license" : "LGPL-3.0-or-later",
	"authors" : [{
			"name" : "Michael Ueberschaer",
			"email" : "info@homepages-mit-zikula.de",
			"homepage" : "https://homepages-mit-zikula.de",
			"role" : "owner"
		}
	],
	"autoload" : {
		"psr-4" : {
			"MU\\FilesModule\\" : ""
		}
	},
	"require" : {
		"php" : ">=5.5.9"
	},
	"require-dev" : {
		"jakub-onderka/php-parallel-lint" : "^1",
		"pdepend/pdepend" : "^2",
		"phploc/phploc" : "^4",
		"phpmd/phpmd" : "^2",
		"phpunit/phpunit" : "^5",
		"sebastian/phpcpd" : "^3",
		"squizlabs/php_codesniffer" : "^3",
		"theseer/phpdox" : "^0"
	},
	"extra" : {
		"zikula" : {
			"core-compatibility" : ">=2.0.11 <3.0.0",
			"class" : "MU\\FilesModule\\MUFilesModule",
			"displayname" : "Files",
			"url" : "files",
			"oldnames" : [
				"MUFiles"
			],
			"capabilities" : {
				"user" : {
					"route" : "mufilesmodule_collection_index"
				},
				"admin" : {
					"route" : "mufilesmodule_collection_adminindex"
				},
				"categorizable" : {
					"entities" : [
						"MU\\FilesModule\\Entity\\CollectionEntity"
					]
				}
			},
			"securityschema" : {
				"MUFilesModule::" : "::",
				"MUFilesModule:ItemListBlock:" : "Block title::",
				"MUFilesModule:ItemBlock:" : "Block title::",
				"MUFilesModule:ModerationBlock:" : "Block title::",
				"MUFilesModule:Collection:" : "Collection ID::",
				"MUFilesModule:Collection:Collection" : "Collection ID:Collection ID:",
				"MUFilesModule:File:" : "File ID::",
				"MUFilesModule:Collection:File" : "Collection ID:File ID:",
				"MUFilesModule::Ajax" : "::"
			}
		}
	},
	"config" : {
		"vendor-dir" : "vendor",
		"preferred-install" : "dist",
		"optimize-autoloader" : true,
		"platform" : {
			"php" : "7.0.30"
		},
		"sort-packages" : true
	}
}