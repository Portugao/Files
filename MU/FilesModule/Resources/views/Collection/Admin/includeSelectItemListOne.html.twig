{# purpose of this template: inclusion template for display of related collection #}
<ul id="{{ idPrefix }}ReferenceList">
{% if item is defined and item.getKey()|default %}
{% set idPrefixItem = idPrefix ~ 'Reference_' ~ item.getKey() %}
<li id="{{ idPrefixItem }}">
    {{ item|mufilesmodule_formattedTitle }}
     <a id="{{ idPrefixItem }}Remove" href="javascript:mUFilesRemoveRelatedItem('{{ idPrefix }}', '{{ item.getKey() }}');"><span class="fa fa-trash-o"></span></a>
</li>
{% endif %}
</ul>
